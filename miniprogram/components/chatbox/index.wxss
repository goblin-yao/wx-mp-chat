/* components/chatcard/chatcard.wxss */
text {
  word-break: break-all;
}

.scenes-text {
  display: inline-block;
  margin: 20rpx auto;
  font-size: 24rpx;
  padding: 8rpx 12rpx;
  background-image: none;
  background-color: rgb(239, 194, 71);
  border-radius: 6rpx;
  color: var(--white);
  line-height: 1.4;
}

.input-status {
  animation: inputStatusFramse 1s infinite steps(1, start);
  -webkit-animation: inputStatusFramse 1s infinite steps(1, start);
}

@keyframes inputStatusFramse {

  0%,
  100% {
    opacity: 0;
  }

  50% {
    opacity: 1;
  }
}

.cu-avatar-user {
  font-size: 32rpx;
  background: #6aa06a;
  border-radius: 40rpx;
}

.cancel_text {
  color: #aaaaaa;
  height: 40px;
  line-height: 40px;
  position: absolute;
  right: -40px;
}

.retry_text {
  color: #aaaaaa;
  height: 44px;
  line-height: 44px;
  position: absolute;
}

.voice-msg-container {
  display: flex;
  align-items: center;
}

.text_link {
  color: rgb(55, 55, 165);
  display: inline-block;
}

.voice-msg-displaying {
  -webkit-animation: voice-icon-animate 2s linear infinite;
  animation: voice-icon-animate 2s linear infinite;
}

@-webkit-keyframes voice-icon-animate {
  10% {
    opacity: .25;
  }

  90% {
    opacity: .75;
  }
}

@keyframes voice-icon-animate {
  10% {
    opacity: .25;
  }

  90% {
    opacity: .75;
  }
}

.voice-msg {
  height: 64rpx;
  width: 64rpx;
  background-size: contain;
  background-image: url('data:image/png;base64,');
}

.chat-ai-avatar {
  background-size: contain;
  background-image: url('data:image/png;base64,');
}

.loading {
  background-size: contain;
  width: 20px;
  height: 20px;
  animation: circle infinite 0.75s linear;
  background-image: url('data:image/png;base64,');
}

@keyframes circle {
  0% {
    transform: rotate(0);
  }

  100% {
    transform: rotate(360deg);
  }
}

.loading-text {
  width: 100rpx;
}

.loading-text::after {
  content: "";
  animation: ld 1.8s infinite;
}

@keyframes ld {

  0%,
  100% {
    content: '';
  }

  25% {
    content: '。';
  }

  50% {
    content: '。。';
  }

  75% {
    content: '。。。';
  }
}