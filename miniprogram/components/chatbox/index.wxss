/* components/chatcard/chatcard.wxss */
text {
  word-break: break-all;
}

.cancel_text {
  color: #aaaaaa;
  height: 40px;
  line-height: 40px;
  position: absolute;
  right: -40px;
}

.retry_text {
  color: #aaaaaa;
  height: 44px;
  line-height: 44px;
  position: absolute;
}

.chat-gpt-avatar {
  background-size: contain;
  background-image: url('data:image/png;base64,');
}

.loading {
  background-size: contain;
  width: 20px;
  height: 20px;
  animation: circle infinite 0.75s linear;
  background-image: url('data:image/png;base64,');
}

@keyframes circle {
  0% {
    transform: rotate(0);
  }

  100% {
    transform: rotate(360deg);
  }
}